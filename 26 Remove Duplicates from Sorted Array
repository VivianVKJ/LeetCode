class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int pre = 0, cur = 0;
        if(nums.empty())
            return 0;
        while(++cur < nums.size()){
            if(nums[pre] != nums[cur])
                nums[++pre] = nums[cur];
            }
        return pre + 1;
    }
};
